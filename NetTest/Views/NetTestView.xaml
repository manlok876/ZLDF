<Window x:Class="NetTest.Views.NetTestView"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:NetTest.Views"
		xmlns:viewmodels="clr-namespace:NetTest.ViewModels"
		d:DataContext="{d:DesignInstance Type=viewmodels:NetTestViewModel}"
		mc:Ignorable="d"
		Title="NetTestView" Height="450" Width="800">
	<Grid Background="#FF2A2A2A">
		<TextBlock Text="{Binding IpListString}" Background="White" TextWrapping="Wrap" Height="50" Width="300" Margin="10,10,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
	</Grid>
</Window>
