<Window x:Class="ZLDF.Scoreboard.Views.ExportFightsTSVDialog"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:ZLDF.Scoreboard.Views"
		mc:Ignorable="d"
		Title="Экспортировать список боёв"
		Height="450" Width="400">
	<DockPanel>
		<Button x:Name="SubmitButton" Content="Ок" Margin="10" Height="50" DockPanel.Dock="Bottom" Click="SubmitButton_Click"/>
		<TextBox x:Name="DuelsTextBox" IsReadOnly="True" AcceptsTab="True" AcceptsReturn="True" TextWrapping="NoWrap" BorderBrush="Green" BorderThickness="5" Margin="5" DockPanel.Dock="Top"/>
	</DockPanel>
</Window>
